layout{
  match:ReturnAction1(this)
  mode (Details)
  
  content{
    section{
      title("#{value(this.foodName)} 효능")
      content{
        list-of (this) {
        //has-details (true)
          where-each (item) {
            layout-macro (list-food-card) {
              param (returnAction1) {
                expression (item)
              }
            }
          }
        }
      }
    }
  }
}